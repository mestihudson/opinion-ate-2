{
  "name": "opinion-ate-2",
  "version": "0.0.0",
  "scripts": {
    "add:version": "sed -i \"s#</title># [build => sha: ${GITHUB_SHA:-HEAD}, id: ${GITHUB_RUN_ID:-1}, number: ${GITHUB_RUN_NUMBER:-1}]</title>#g\" dist/*/index.html",
    "replace:api-hash-token": "sed -i \"s#API_HASH_TOKEN#${API_HASH_TOKEN:-ApiHashToken}#g\" dist/*/main.*.js",
    "predeploy": "npm run build -- --base-href=./ && npm run add:version && npm run replace:api-hash-token",
    "deploy": "gh-pages -d dist/opinion-ate-2",
    "ng": "ng",
    "prestart": "[ ! -e src/environments/environment.development.ts ] && cp -frv src/environments/environment.development.ts.template src/environments/environment.development.ts || echo ok",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "e2e": "npx playwright test",
    "ci:e2e": "start-server-and-test 'npm start' http://localhost:4200 'E2E_BASE_URL=http://localhost:4200 npm run e2e'",
    "test": "npx jest",
    "lint": "ng lint",
    "prepare": "cd .. && npx husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@ngrx/effects": "^16.3.0",
    "@ngrx/store": "^16.3.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.4",
    "@angular-eslint/builder": "16.2.0",
    "@angular-eslint/eslint-plugin": "16.2.0",
    "@angular-eslint/eslint-plugin-template": "16.2.0",
    "@angular-eslint/schematics": "16.2.0",
    "@angular-eslint/template-parser": "16.2.0",
    "@angular/cli": "^16.2.4",
    "@angular/compiler-cli": "^16.2.0",
    "@playwright/test": "^1.39.0",
    "@types/jest": "^29.5.6",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "gh-pages": "^6.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-preset-angular": "^13.1.2",
    "prettier": "^3.0.3",
    "prettier-eslint": "^16.1.1",
    "start-server-and-test": "^2.0.1",
    "typescript": "~5.1.3"
  }
}
